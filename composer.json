{
    "name": "myTaste/recipe-game",
    "description": "Recipe Game",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "myTaste"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/203webgroup/phockito.git",
            "type": "git"
        }
    ],
    "require": {
        "silex/silex": "~1.2",
        "doctrine/dbal": "2.*",
        "dflydev/doctrine-orm-service-provider": "1.0.*@dev"
    },
    "require-dev": {
        "tnt/phockito": "1.1.1",
        "phpunit/phpunit": "3.7.*",
        "phpunit/phpunit-selenium": "1.3.3",
        "behat/behat": "2.4.*@stable",
        "behat/mink": "1.4@stable",
        "behat/mink-extension": "*",
        "behat/mink-selenium2-driver": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-sahi-driver": "*",
        "sanpi/behatch-contexts": "*",
        "squizlabs/php_codesniffer": "1.5.2",
        "phpmd/phpmd": "1.4.*"
    },
    "autoload": {
        "psr-4": {
            "Tnt\\": "src/Tnt",
            "Tests\\Tnt\\": "tests/Tnt"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}